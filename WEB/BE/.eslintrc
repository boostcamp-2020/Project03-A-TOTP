{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb",
    "eslint:recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "ignorePatterns": [
    "node_modules/"
  ],

  "rules": {
    "no-console": "error",
    "camelcase": "error",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}